{
  "name": "@darkresearch/mallory-client",
  "version": "0.1.0",
  "description": "Opinionated react native crypto x AI chat app boilerplate with wallet support, conversational AI, dynamic UI, and x402 support",
  "main": "index.ts",
  "scripts": {
    "start": "bun run generate-version && expo start",
    "android": "bun run generate-version && expo run:android",
    "ios": "bun run generate-version && expo run:ios",
    "web": "bun run generate-version && expo start --web",
    "generate-version": "bun scripts/generate-version.js",
    "web:export": "bun run generate-version && EXPO_PLATFORM=web expo export --platform web",
    "prebuild": "bun run generate-version && expo prebuild",
    "build:android": "bun run generate-version && eas build --platform android",
    "build:ios": "bun run generate-version && eas build --platform ios",
    "postinstall": "bun run generate-version",
    "type-check": "tsc --noEmit",
    "test": "bun test",
    "test:unit": "bun test __tests__/unit/",
    "test:unit:auth": "bun test __tests__/unit/AuthContext.test.tsx",
    "test:unit:grid": "bun test __tests__/unit/GridContext.test.tsx",
    "test:unit:otp": "bun test __tests__/unit/VerifyOtpScreen.test.tsx",
    "test:unit:wallet": "bun test __tests__/unit/WalletDataService.test.ts",
    "test:unit:grid-mount": "bun test __tests__/unit/GridContextMount.test.tsx",
    "test:unit:draft": "bun test __tests__/unit/draftMessages.test.ts",
    "test:unit:chat-input": "bun test __tests__/unit/ChatInput.test.tsx",
    "test:integration": "bun test __tests__/integration/",
    "test:integration:auth": "bun test __tests__/integration/auth-grid-integration.test.ts",
    "test:integration:otp": "bun test __tests__/integration/otp-screen-grid-integration.test.ts",
    "test:integration:session": "bun test __tests__/integration/session-persistence.test.ts",
    "test:integration:wallet": "bun test __tests__/integration/wallet-grid-integration.test.ts",
    "test:integration:chat": "bun test __tests__/integration/chat-flow-updated.test.ts",
    "test:integration:chat-history": "bun test __tests__/integration/chat-history-loading.test.ts",
    "test:e2e": "bun test __tests__/e2e/",
    "test:e2e:auth": "bun test __tests__/e2e/auth-flows.test.ts",
    "test:e2e:persistence": "bun test __tests__/e2e/otp-flow-persistence.test.ts",
    "test:e2e:chat": "bun test __tests__/e2e/chat-user-journey-updated.test.ts",
    "test:e2e:chat-history": "bun test __tests__/e2e/chat-history-journey.test.ts",
    "test:signup": "bun test __tests__/e2e/signup-flow.test.ts",
    "test:signup:errors": "bun test __tests__/e2e/signup-error-scenarios.test.ts",
    "test:all": "bun run test:unit && bun run test:integration && bun run test:e2e",
    "test:auth:all": "bun run test:unit:auth && bun run test:unit:grid && bun run test:unit:otp && bun run test:integration:auth && bun run test:integration:otp && bun run test:e2e:auth",
    "test:chat:all": "bun run test:unit:draft && bun run test:unit:chat-input && bun run test:integration:chat && bun run test:e2e:chat",
    "test:chat-history:all": "bun run test:integration:chat-history && bun run test:e2e:chat-history",
    "test:ui": "playwright test",
    "test:ui:headed": "playwright test --headed",
    "test:ui:debug": "playwright test --debug",
    "test:ui:signup": "playwright test signup.spec.ts",
    "test:ui:bulk": "playwright test bulk-signup.spec.ts",
    "test:ui:bulk:10": "NUM_USERS=10 playwright test bulk-signup.spec.ts --workers=5",
    "test:ui:bulk:100": "NUM_USERS=100 playwright test bulk-signup.spec.ts --workers=10",
    "test:ui:report": "playwright show-report",
    "test:setup": "bun __tests__/scripts/setup-test-account.ts",
    "test:balance": "bun __tests__/scripts/check-balance.ts",
    "test:validate:storage": "bun __tests__/scripts/validate-storage.ts",
    "test:validate:mailosaur": "bun __tests__/scripts/validate-mailosaur.ts",
    "test:validate:auth": "bun __tests__/scripts/validate-auth.ts",
    "test:validate:grid": "bun __tests__/scripts/validate-grid.ts",
    "test:validate:grid-load": "bun __tests__/scripts/validate-grid-load.ts",
    "test:validate:conversation": "bun __tests__/scripts/validate-conversation.ts",
    "test:validate:chat": "bun __tests__/scripts/validate-chat-api.ts",
    "test:validate:ephemeral": "bun __tests__/scripts/validate-ephemeral-wallet.ts",
    "test:validate:payment": "bun __tests__/scripts/validate-payment-detection.ts",
    "test:validate:all": "bun __tests__/scripts/validate-storage.ts && bun __tests__/scripts/validate-mailosaur.ts && bun __tests__/scripts/validate-auth.ts && bun __tests__/scripts/validate-grid-load.ts && bun __tests__/scripts/validate-conversation.ts",
    "test:grid": "bun test __tests__/e2e/grid-payment.test.ts",
    "test:x402": "bun test __tests__/e2e/x402-complete.test.ts",
    "test:x402:full": "bun test __tests__/e2e/x402-full-flow.test.ts",
    "test:x402:nansen": "bun test __tests__/e2e/x402-nansen-true-e2e.test.ts --timeout 300000",
    "test:x402:nansen:all": "bun test __tests__/e2e/x402-nansen-all-endpoints.test.ts --timeout 900000",
    "test:e2e:all": "bun run test:validate:all && bun run test:x402 && bun run test:x402:nansen"
  },
  "dependencies": {
