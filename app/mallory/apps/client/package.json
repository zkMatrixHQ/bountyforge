    "@solana/web3.js": "^1.98.4",
    "@sqds/grid": "0.1.2",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@supabase/supabase-js": "^2.51.0",
    "@types/uuid": "^10.0.0",
    "@ungap/structured-clone": "^1.3.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^5.0.50",
    "concurrently": "^8.2.2",
